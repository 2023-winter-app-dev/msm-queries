<table border = 1">
<tr>
  <th>
    ID
  </th>
  
  <th>
    Title
  </th>
  
  <th>
    Image
  </th>
  
  <th>
    Year
  </th>

  <th>
    Director
  </th>

  <th>
  </th>
</tr>

<% Movie.all.each do |listing| %>
<tr>
  <th>
    <%= listing.id %>
  </th>

  <th>
    <%= listing.title %>
  </th>

  <th>
    <img src = <%= listing.image %>>
  </th>

  <th>
    <%= listing.year %>
  </th>

  <th>
    <%= Director.all.where({:id => listing.director_id})[0].name%>
  </th>

  <th>
    <a href= <%= "/movies/"+listing.id.to_s%>>Show details </a>
  </th>
</tr>

<% end %>

</table>
